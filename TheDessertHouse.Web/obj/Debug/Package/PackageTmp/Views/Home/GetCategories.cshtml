@model IEnumerable<TheDessertHouse.Web.Models.CategoryView>>
<div id="news" class="boxed">
		<h2 class="title">Categories</h2>
		<div class="content">
			<ul>
			<li class="first"><a href="/">All</a></li>
			@foreach (var cat in Model)
	{
		 <li>@Html.ActionLink(cat.Title,"AllArticles",new{controller="Home",category=cat.Path})</li>
	}
			</ul>
		</div>
	</div>
